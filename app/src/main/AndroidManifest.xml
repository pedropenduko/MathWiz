<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rommel.mathwiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MathWiz">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MathWizResult" />
        <activity
            android:name=".MatchWizAbout"
            android:label="@string/title_activity_match_wiz_about" />
        <activity
            android:name=".MathWizAbout"
            android:label="@string/title_activity_math_wiz_about"></activity>
    </application>

</manifest>